
<table class="table table-responsive" id="health_care_table">
  <thead>
  <tr>
    <th>Name</th>
    <th>Health care facility type</th>
    <th>Health care facility status</th>
    <th>Health care facility contact</th>
    <th>Date started</th>
    <th>Date end</th>
  </tr>
  </thead>

  <tbody>
  <% @health_care_facilities.each do |health_care_facility| %>
      <tr>
        <td><%= link_to health_care_facility.name, health_care_facility %></td>
        <td><%= health_care_facility.health_care_facility_type %></td>
        <td><%= health_care_facility.health_care_facility_status %></td>
        <td><%= health_care_facility.health_care_facility_contact %></td>
        <td><%= format_date health_care_facility.date_started %></td>
        <td><%= format_date health_care_facility.date_end %></td>
  </tr>
  <% end %>
  </tbody>
</table>
<script>
  $(document).ready(function(){
    initDataTable('health_care_table')
  })
</script>
