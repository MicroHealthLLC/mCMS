<%= I18n.t @enumeration.option_name %>

<%=form_for :enumeration, object: @enumeration, :url => enumerations_path, html: { class: 'form-horizontal' } do |f| %>
  <%= f.hidden_field :type  %>
  <%= render :partial => 'form', :locals => {:f => f} %>

    <div class="form-group" >
      <div class="col-md-11">
        <%= link_to t('button_cancel'), enumerations_path, class: 'btn btn-warning pull-right' %>
        <%= f.submit t(:button_create), class: 'btn btn-success pull-right' %>
      </div>
    </div>
<% end %>
