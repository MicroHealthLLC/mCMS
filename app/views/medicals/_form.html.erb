<%= simple_form_for(@medical) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :user_id %>
    <%= f.input :icdcm_code_id %>
    <%= f.input :medical_facility %>
    <%= f.input :date_of_diagnosis %>
    <%= f.input :medical_history_status_id %>
    <%= f.input :medical_history_type_id %>
    <%= f.input :description %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
