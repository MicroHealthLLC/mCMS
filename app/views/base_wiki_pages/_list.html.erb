<div class="row">
  <div class="col-xs-12" style="padding:15px 25px 15px 25px;">
    <ul>
      <% @pages.each do |page| -%>
          <li><%= link_to page.title, wiki_page_path( page ) %></li>
          <%= render_subpages(page).html_safe %>
      <% end -%>

      <%= will_paginate @pages, renderer: BootstrapPagination::Rails %>
    </ul>
  </div>
</div>