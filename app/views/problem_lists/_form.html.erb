<%= simple_form_for(@problem_list) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :icdcm_code_id %>
    <%= f.input :user_id %>
    <%= f.input :date_onset %>
    <%= f.input :date_resolved %>
    <%= f.input :problem_status_id %>
    <%= f.input :problem_type_id %>
    <%= f.input :description %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
