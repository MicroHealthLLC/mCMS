<div class="row">
  <div class="col-xs-12" style="padding:15px 25px 15px 25px;">
    <p>
      <strong><%= t('case') %>:</strong>
      <%= link_to_case @enrollment.case %>
    </p>
    <p>
      <strong>Name:</strong>
      <%= @enrollment.name %>
    </p>

     <p>
      <strong>Location:</strong>
      <%= @enrollment.enrollment_location %>
    </p>

    <p>
      <strong>Enrollment type:</strong>
      <%= @enrollment.enrollment_type %>
    </p>

    <p>
      <strong>Enrollment Status:</strong>
      <%= @enrollment.enrollment_status %>
    </p>

    <p>
      <strong>Date start:</strong>
      <%= format_date @enrollment.date_start %>
    </p>

    <p>
      <strong>Date end:</strong>
      <%= format_date @enrollment.date_end %>
    </p>

    <p>
      <strong>Note:</strong>
      <%= @enrollment.note.html_safe %>
    </p>

    <p>
      <strong><%= t('attachments') %>:</strong>
      <% @enrollment.enrollment_attachments.each do |attachment| %>
          <%= render 'attachments/attachment_show_only', :attachment => attachment %>
      <% end %>
    </p>
  </div>
</div>
