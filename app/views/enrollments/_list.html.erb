<table class="table table-bordered table-responsive" id="enrollments_table">
  <thead>
  <tr>
    <th>Name</th>
    <th>Enrollment Type</th>
    <th>Enrollment Status</th>
    <th>Date start</th>
    <th>Date end</th>
  </tr>
  </thead>

  <tbody>
  <% enrollments.each do |enrollment| %>
      <tr>

        <td><%= link_to enrollment.name, enrollment %></td>
        <td><%= enrollment.enrollment_type %></td>
        <td><%= enrollment.enrollment_status %></td>
        <td><%= enrollment.date_start %></td>
        <td><%= enrollment.date_end %></td>
      </tr>
  <% end %>
  </tbody>
</table>

<script>
    $(document).ready(function(){
        initDataTable('enrollments_table')
    } );
</script>
