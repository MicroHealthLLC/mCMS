<table class="table table-responsive" id="financials_table">
  <thead>
  <tr>
    <th>Title</th>
    <th>Financial type</th>
    <th>Financial status</th>
    <th>Financial state</th>
    <th>Estimated amount</th>
    <th>Date start</th>
    <th>Date end</th>

  </tr>
  </thead>

  <tbody>
  <% @financials.each do |financial| %>
      <tr>
        <td><%= link_to financial.title, financial %></td>
        <td><%= financial.financial_type %></td>
        <td><%= financial.financial_status %></td>
        <td><%= financial.financial_state %></td>
        <td><%= financial.estimated_amount %></td>
        <td><%= financial.date_start %></td>
        <td><%= financial.date_end %></td>
       </tr>
  <% end %>
  </tbody>
</table>
<script>
  $(document).ready(function(){
    initDataTable('financials_table')
  })
</script>
