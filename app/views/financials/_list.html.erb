<table class="table table-responsive" id="financials_table">
  <thead>
  <tr>
    <th>Snomed</th>
    <th>Financial status</th>
    <th>Date start</th>
    <th>Date end</th>

  </tr>
  </thead>

  <tbody>
  <% @financials.each do |financial| %>
      <tr>
        <td><%= link_to financial.snomed, financial %></td>
        <td><%= financial.financial_status %></td>
        <td><%= format_date financial.date_start %></td>
        <td><%= format_date financial.date_end %></td>
       </tr>
  <% end %>
  </tbody>
</table>
<script>
  $(document).ready(function(){
    initDataTable('financials_table')
  })
</script>
