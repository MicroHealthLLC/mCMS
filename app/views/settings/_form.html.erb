<div id="tabs">
  <ul>
    <li><a id="link-tabs-general" href="#tabs-home" >Home page</a></li>
    <li><a id="link-tabs-modules" href="#tabs-module">Module page</a></li>
    <li><a id="link-tabs-modules" href="#tabs-theme">Themes</a></li>
    <li><a id="link-tabs-modules" href="#tabs-authentification">Authentication</a></li>
    <li><a id="link-tabs-modules" href="#tabs-notification">Notifications</a></li>
    <li><a id="link-tabs-modules" href="#tabs-keys">Keys</a></li>
  </ul>
  <div id='tabs-home' >
    <%= render partial: 'general_setting' %>
  </div>

  <div id='tabs-module' >
    <div class="row">
      <%= form_tag(set_modules_settings_path, method: 'post',  class: 'form-horizontal' ) do |f| %>
          <% EnabledModule.order('name ASC').each do |em| %>
              <div class="col-xs-12">
                <%= check_box_tag em.name, em.status, em.status %> <%= t(em.name) %>
              </div>
          <% end %>
          <div class="form-group margin-top-10" >
            <div class="col-xs-12">
              <%= submit_tag t('button_save'), class: 'btn btn-primary pull-right' %>
            </div>
          </div>
      <% end %>
    </div>
  </div>
  <div id='tabs-theme' >
    <div class="row">
      <div class="col-xs-12">
        <%= render partial: 'cms_theme' %>
      </div>
    </div>
  </div>
  <div id='tabs-authentification' >
    <div class="row">
      <div class="col-xs-12">
        <%= render partial: 'cms_authentification' %>
      </div>
    </div>
  </div>
  <div id='tabs-notification' >
    <div class="row">
      <div class="col-xs-12">
        <%= render partial: 'cms_email' %>
      </div>
    </div>
  </div>
  <div id='tabs-keys' >
    <div class="row">
      <div class="col-xs-12">
        <%= render partial: 'keys' %>
      </div>
    </div>
  </div>





</div>
<script>
  $( function() {
    $tabs = $( "#tabs" ).tabs();
  } );
</script>