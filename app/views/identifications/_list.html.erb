<table class="table  table-responsive" id="identification_table"  style="width: 100%">
  <thead>
  <tr>
    <th><%= t(:identification_type_id) %></th>
    <th><%= t(:identification_number) %></th>
    <th><%= t(:status) %></th>
    <th><%= t(:date_expired) %></th>
    <th><%= t(:issued_by_type) %></th>
    <th><%= t(:date_issued) %></th>
  </tr>
  </thead>

  <tbody>
  </tbody>
</table>
<% columns= [  { "searchable": true, "orderable": true },
                { "searchable": true, "orderable": true },
                { "searchable": true, "orderable": true },
                { "searchable": true, "orderable": true },
                { "searchable": true, "orderable": true },
                { "searchable": true, "orderable": true }]
%>
<%= render partial: 'datatable_script', locals: {
    model_table: 'identification_table',
    url: identifications_path(format: :csv, status_type: params[:status_type]),
    columns: columns.flatten,
    ajax_url: identifications_path(format: :json, status_type: params[:status_type])
} %>

