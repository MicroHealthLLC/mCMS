<%= simple_form_for(@award) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :user_id %>
    <%= f.input :award_type_id %>
    <%= f.input :award_enum_id %>
    <%= f.input :award_date %>
    <%= f.input :note %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
