<p id="notice"><%= notice %></p>

<h1>Awards</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Award type</th>
      <th>Award enum</th>
      <th>Award date</th>
      <th>Note</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @awards.each do |award| %>
      <tr>
        <td><%= award.user_id %></td>
        <td><%= award.award_type_id %></td>
        <td><%= award.award_enum_id %></td>
        <td><%= award.award_date %></td>
        <td><%= award.note %></td>
        <td><%= link_to 'Show', award %></td>
        <td><%= link_to 'Edit', edit_award_path(award) %></td>
        <td><%= link_to 'Destroy', award, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Award', new_award_path %>
