<table class="table table-responsive" id="deployment_history_table">
  <thead>
  <tr>
    <th>Deployment operation</th>
    <th>Location</th>
    <th>City</th>
    <th>State</th>
    <th>Country</th>
    <th>Date start</th>
    <th>Date end</th>
  
  </tr>
  </thead>

  <tbody>
  <% @deployment_histories.each do |deployment_history| %>
      <tr>
      
        <td><%= link_to deployment_history.deployment_operation, deployment_history %></td>
        <td><%= deployment_history.location %></td>
        <td><%= deployment_history.city %></td>
        <td><%= deployment_history.state %></td>
        <td><%= deployment_history.country_type %></td>
        <td><%= deployment_history.date_start %></td>
        <td><%= deployment_history.date_end %></td>
       </tr>
  <% end %>
  </tbody>
</table>
<script>
  $(document).ready(function(){
    initDataTable('deployment_history_table')
  })
</script>