<p id="notice"><%= notice %></p>

<h1>Service Histories</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Rank</th>
      <th>Service type</th>
      <th>Service status</th>
      <th>Component</th>
      <th>Discharge type</th>
      <th>Date entered</th>
      <th>End active obliged service</th>
      <th>Demobilization</th>
      <th>Separation</th>
      <th>Temporary disability retirement list</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @service_histories.each do |service_history| %>
      <tr>
        <td><%= service_history.user_id %></td>
        <td><%= service_history.rank_id %></td>
        <td><%= service_history.service_type_id %></td>
        <td><%= service_history.service_status_id %></td>
        <td><%= service_history.component_id %></td>
        <td><%= service_history.discharge_type_id %></td>
        <td><%= service_history.date_entered %></td>
        <td><%= service_history.end_active_obliged_service %></td>
        <td><%= service_history.demobilization %></td>
        <td><%= service_history.separation %></td>
        <td><%= service_history.temporary_disability_retirement_list %></td>
        <td><%= link_to 'Show', service_history %></td>
        <td><%= link_to 'Edit', edit_service_history_path(service_history) %></td>
        <td><%= link_to 'Destroy', service_history, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Service History', new_service_history_path %>
