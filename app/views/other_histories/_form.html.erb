<%= simple_form_for(@other_history) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :user_id %>
    <%= f.input :icdcm_code_id %>
    <%= f.input :date_identified %>
    <%= f.input :date_resolved %>
    <%= f.input :other_history_status_id %>
    <%= f.input :other_history_type_id %>
    <%= f.input :description %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
