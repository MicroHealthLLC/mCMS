<%= simple_form_for(@family_history) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :user_id %>
    <%= f.input :icdcm_code_id %>
    <%= f.input :date_identified %>
    <%= f.input :family_status_id %>
    <%= f.input :family_type_id %>
    <%= f.input :description %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
